group setting keepalive "iDigi Connector Keepalive Configuration"
    element rx "Interval which device will send Keep-Alive messages: Between 5 to 7200 seconds" type uint32 min 5 max 7200 unit "in seconds"
    element tx "Interval which server will send Keep-Alive messages: Between 5 to 7200 seconds" type uint32 min 5 max 7200 unit "in seconds"
    
